
docker run -d -h node1 -v /mnt:/data \
    -p 192.168.0.117:8300:8300 \
    -p 192.168.0.117:8301:8301 \
    -p 192.168.0.117:8301:8301/udp \
    -p 192.168.0.117:8302:8302 \
    -p 192.168.0.117:8302:8302/udp \
    -p 192.168.0.117:8400:8400 \
    -p 192.168.0.117:8500:8500 \
    -p 172.17.0.1:53:53/udp \
    progrium/consul -server -advertise 192.168.0.117 -bootstrap-expect 3

docker run -d -h node2 -v /mnt:/data  \
    -p 192.168.0.110:8300:8300 \
    -p 192.168.0.110:8301:8301 \
    -p 192.168.0.110:8301:8301/udp \
    -p 192.168.0.110:8302:8302 \
    -p 192.168.0.110:8302:8302/udp \
    -p 192.168.0.110:8400:8400 \
    -p 192.168.0.110:8500:8500 \
    -p 172.17.0.1:53:53/udp \
    progrium/consul -server -advertise 192.168.0.110 -join 192.168.0.117

docker run -d -h node3 -v /mnt:/data  \
    -p 192.168.0.119:8300:8300 \
    -p 192.168.0.119:8301:8301 \
    -p 192.168.0.119:8301:8301/udp \
    -p 192.168.0.119:8302:8302 \
    -p 192.168.0.119:8302:8302/udp \
    -p 192.168.0.119:8400:8400 \
    -p 192.168.0.119:8500:8500 \
    -p 172.17.0.1:53:53/udp \
    progrium/consul -server -advertise 192.168.0.119 -join 192.168.0.117